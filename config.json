{
  "input": {
    "experiments-dir": "path/to/runs/directory",
    "log-suffix": ".log"
  },
  "parse": {
    "override": false,
    "keywords": ["CONTEXT", "INPUT", "RESULT"],
    "result": {
      "format": "JSON",
      "name": "",
      "suffix": ".json"
    }
  },
  "aggregate": {
    "path.to.value.list": "success_percentage"
  },
  "gather": {
    "column-key-mapping": {
      "column_name": "path.to.value.in.result.json"
    }
  },
  "plots": {
    "R-command": "RScript",
    "script": "R/create_plots.R"
  }
}